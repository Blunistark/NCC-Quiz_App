{
  "expo": {
    "scheme": "acme",
    "name": "QuizApp-NCC",
    "slug": "quizapp-ncc",
    "icon": "./assets/ncc-logo.png", // Path to your logo
    "plugins": [
      "expo-router"
    ],
    "android": {
      "package": "com.quizapp.ncc"
    },
    "ios": {
      "bundleIdentifier": "com.quizapp.ncc"
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5836b7b7-61f8-4b74-a17d-deeeaa98f9b0"
      },
      "newArchEnabled": true
    },
    "owner": "blunistark",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "your-update-url"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/*"
    ]
  }
}
